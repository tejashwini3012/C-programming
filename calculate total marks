# include<stdio.h>
int main()
{
    int marks,total_marks=0,percentage_marks;
    for(int i=0;i<5;i++)
    {
    printf("enter the marks of each sub%d\n",i+1);
    scanf("%d",&marks);
    total_marks+=marks;
    }
    percentage_marks=(total_marks*100)/500;
    printf("total marks=%d\n",total_marks);
    printf("percentage marks=%d\n",percentage_marks);
    return 0;
}
